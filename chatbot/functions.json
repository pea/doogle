{
  "lightsOn": {
    "type": "command",
    "prompt": "force turn on the lights",
    "triggerWords": ["lights"],
    "command": ".venv/bin/python3 functions/transmit.py 4543573 -p 433 && python3 functions/transmit.py 4527445 -p 433"
  },
  "lightsOff": {
    "type": "command",
    "prompt": "force turn off the lights",
    "triggerWords": ["lights"],
    "command": ".venv/bin/python3 functions/transmit.py 4543572 -p 433 && python3 functions/transmit.py 4527444 -p 433"
  },
  "playNtsRadioOne": {
    "type": "command",
    "prompt": "play nts radio one",
    "triggerWords": ["radio", "play", "nts"],
    "command": "pkill vlc; pkill VLC; sleep 1; vlc --intf rc --rc-host localhost:12345 http://stream-relay-geo.ntslive.net/stream &"
  },
  "playNtsRadioTwo": {
    "type": "command",
    "prompt": "play nts radio two",
    "triggerWords": ["radio", "play", "nts"],
    "command": "pkill vlc; pkill VLC; sleep 1; vlc --intf rc --rc-host localhost:12345 http://stream-relay-geo.ntslive.net/stream2 &"
  },
  "playNtsRadioExpansions": {
    "type": "command",
    "prompt": "play nts radio expansions",
    "triggerWords": ["radio", "play", "nts"],
    "command": "pkill vlc; pkill VLC; sleep 1; vlc --intf rc --rc-host localhost:12345 https://stream-mixtape-geo.ntslive.net/mixtape3?client=NTSWebApp&t=1706890892205 &"
  },
  "playJazzFm": {
    "type": "command",
    "prompt": "play jazz fm",
    "triggerWords": ["radio", "play", "jazz"],
    "command": "pkill vlc; pkill VLC; sleep 1; vlc --intf rc --rc-host localhost:12345 https://edge-bauerall-01-gos2.sharp-stream.com/jazzhigh.aac &"
  },
  "stopRadio": {
    "type": "command",
    "prompt": "stop radio",
    "triggerWords": ["radio", "stop"],
    "command": "pkill vlc; pkill VLC"
  },
  "time": {
    "type": "environment",
    "prompt": "the current time IS available. The current time IS set to [function_response]",
    "triggerWords": ["time"],
    "command": "python3 functions/time.py"
  },
  "volume": {
    "type": "command",
    "prompt": "change the volume by setting option to the chosen number (0-256)",
    "triggerWords": ["volume"],
    "command": "python3 functions/volume.py --volume [option]",
    "optionExample": "50"
  },
  "restart": {
    "type": "command",
    "prompt": "restart the system",
    "triggerWords": ["restart"],
    "command": "python3 functions/restart.py"
  },
  "tenMinuteTimer": {
    "type": "wakeword",
    "model": "ten_minute_timer",
    "command": "python3 functions/timer.py --time 600"
  },
  "turnTheLightsOn": {
    "type": "wakeword",
    "model": "turn_the_lights_on",
    "command": ".venv/bin/python3 functions/transmit.py 4543573 -p 433 && python3 functions/transmit.py 4527445 -p 433"
  },
  "turnTheLightsOff": {
    "type": "wakeword",
    "model": "turn_the_lights_off",
    "command": ".venv/bin/python3 functions/transmit.py 4543572 -p 433 && python3 functions/transmit.py 4527444 -p 433"
  }
}
